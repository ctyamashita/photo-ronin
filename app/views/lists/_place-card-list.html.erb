<div class="d-flex justify-content-between align-items-center background px-2">
  <div class="avatar-large">
    <img class="small-img" src=<%= place.photos.any? ? cl_image_path(place.photos.first.photo.key) : '/assets/Ronin.jpg' %> /></div>
  <div class="mt-3 me-2">
    <h3 class="bold"><%= place.name %></h3>
    <h4 class="green mb-3"> <%= place.address %> </h4>
  </div>
  <div>
    <h4 class="rating-size">5.0  <i class="fas fa-star green"></i></h4>
  </div>
</div>
